{
    "root": true,
    "extends": [
        "@nuxtjs/eslint-config-typescript",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "no-console": "off",
        "vue/no-setup-props-destructure": "off",
        "prettier/prettier": [
            "warn",
            {
                "semi": true,
                "arrowParens": "always",
                "singleQuote": false,
                "endOfLine": "auto",
                "trailingComma": "es5"
            }
        ]
    }
}